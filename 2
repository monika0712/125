# Step 1

class Animal:
    def make_sound(self):
        print("Generic animal sound")

# Step 2

class Dog(Animal):
    pass

class Cat(Animal):
    pass

# Step 3

class Dog(Animal):
    def make_sound(self):
        print("Bark")

class Cat(Animal):
    def make_sound(self):
        print("Meow")

# Step 4

def animal_sound_in_zoo(animal):
    animal.make_sound()

# Step 5

my_dog = Dog()
my_cat = Cat()

animal_sound_in_zoo(my_dog) # Output: Bark
animal_sound_in_zoo(my_cat) # Output: Meow
